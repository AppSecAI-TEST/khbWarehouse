@charset "utf-8";
/*
清除样式（重置CSS 定位 清除浮动 长度高度 边距）
*/
/*CSS重置*/
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, form, fieldset, legend, input, button, textarea, select, p, blockquote, th, td ,table, a , span, i, em, label , img{ margin: 0; padding: 0;}
article, aside, details, figcaption, figure, footer, header, hgroup, nav, section, summary {  display: block;}
audio, canvas, progress, video { display: inline-block; vertical-align: baseline; }
address, button, caption, em, input, optgroup, option, select, strong, textarea, th {  font: inherit;}
button, input, optgroup, option, select, textarea { outline: medium none; font-size:100%;  vertical-align:middle;}
abbr, acronym { border: 0 none; font-variant: normal;}
sup { vertical-align: baseline;}
sub { vertical-align: baseline;}
legend { color: #000;}
table { border-collapse: collapse;  border-spacing: 0;}
i,b,em{ font-style: normal; font-weight: normal;}
fieldset, img , input { border: 0 none;}
img{  vertical-align:middle;  -ms-interpolation-mode:bicubic;}
li { list-style: outside none none;}
caption, th { text-align: left;}
h1, h2, h3, h4, h5, h6 { font-size: 100%; font-weight: normal;}
form{ display:inline;}
textarea{ resize:none;}
/*input[type="text"], input[type="tel"] { -webkit-tap-highlight-color: rgba(0, 0, 0, 0);}*/
/* 去除iphone ipad 设备默认按钮样式 */
button,input[type="button"], input[type="reset"], input[type="submit"],input[type="date"],input[type="text"] { cursor:pointer; -webkit-appearance:button; -moz-appearance:button; border-radius: 0;}
input::-webkit-outer-spin-button, input::-webkit-inner-spin-button { -webkit-appearance: none !important; margin: 0;}
/*input::-moz-placeholder, textarea::-moz-placeholder { color: #a2a2a2; }*/
/*input:-ms-input-placeholder, textarea:-ms-input-placeholder { color: #a2a2a2; }*/
/*input::-webkit-input-placeholder, textarea::-webkit-input-placeholder { color: #a2a2a2; }*/
/*input:focus:-moz-placeholder, input:focus::-webkit-input-placeholder { color:transparent;}*/
button::-moz-focus-inner, input::-moz-focus-inner { padding:0; border:0}
iframe{
    border: 0;
    padding: 0;
    margin: 0;
}
.clearfix:after {
    clear: both;
    content: ".";
    display: block;
    line-height: 0;
    height: 0;
    overflow: hidden;
}
.clearfix { zoom:1;}
.ellipsis {
    white-space:nowrap;
    text-overflow:ellipsis;
    overflow:hidden
}
/*------基础部分------*/
html {
    font-size: 50px;
    color: #666;
    font-family: 'STHeiti Light','Microsoft Yahei', 'Helvetica Neue', Helvetica, STHeiTi, Arial, sans-serif;
    min-width: 320px;
    height: 100%;
    -webkit-tap-highlight-color: transparent;
}

body {
    height: 100%;
    overflow-x: hidden;
    -webkit-user-select: none;
    -webkit-text-size-adjust: none;
    -webkit-overflow-scrolling: touch;
    -moz-user-select: none;
    font-size: 14px;
    background: #eee;
}
a {
    color: #333;
    text-decoration: none;
    outline: none;
    -webkit-tap-highlight-color: transparent;
}
