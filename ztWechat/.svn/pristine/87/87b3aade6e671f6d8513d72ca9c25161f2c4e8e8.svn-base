#wrapper {
    position:absolute;
    z-index:1;
    top: 200px;
    bottom: 0px;
    left: 0px;
    width: 100%;
    overflow: hidden;
}

#scroller {
    position:absolute;
    z-index: 1;
    /*	-webkit-touch-callout:none;*/
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    width: 92%;
    padding: 0 4%;
    margin-bottom:4%;
}

