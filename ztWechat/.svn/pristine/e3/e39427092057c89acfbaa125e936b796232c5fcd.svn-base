/*---首页---*/
/*ul wrapper*/
.banner {
    position: relative;
    height: 2rem;
    overflow: hidden;
}
.banner240{
    height: 3rem;
}
.banner img {
    width: 100%;
    height: 100%;
}
.banner .islider-dot {
    width: 0.6em;
    height: 0.6em;
}
.banner240 .islider-dot {
    width: 0.4em;
    height: 0.4em;
}
#iSlider-wrapper {
    height: 100%;
    width: 100%;
    overflow: hidden;
    position: absolute;
}

#iSlider-wrapper ul {
    list-style: none;
    margin: 0;
    padding: 0;
    height: 100%;
    overflow: hidden;
}
#iSlider-wrapper li {
    position: absolute;
    margin: 0;
    padding: 0;
    height: 100%;
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-pack: center;
    -webkit-box-align: center;
    list-style: none;
}
#iSlider-wrapper li  img{
    max-width: 100%;
    max-height: 100%;
}
.banner .islider-dot-wrap{
    z-index: 999;
}
.banner .islider-dot.active{
    background-color: #FF7300;
    border: 1px solid #FF7300;
}
