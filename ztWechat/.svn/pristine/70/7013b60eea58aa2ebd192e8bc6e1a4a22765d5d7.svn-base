}

.scroll .lotteryScroll div {
    float: left;
}

.scroll .lotteryScroll ul {
    float: left;
    height: .8rem;
    overflow: hidden;
    zoom: 1;
}

.scroll .lotteryScroll ul li {
    float: left;
    line-height: .8rem;
    margin-right: 1rem;
}
/* 大转盘样式 */
.turntable-bg {
    z-index:20;
    width: 90%;
    left:50%;
    transform: translateX(-50%);
    position: absolute;
    top:12%;
    background-size:100% 100%;
}
.turntable-bg img {
    width:100%;
    text-align: center;
    margin: 0 auto;
}
.turntable-bg .pointer {
    width: 29%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform:translateX(-50%) translateY(-57%);
    -webkit-transform:translateX(-50%) translateY(-57%);
    -moz-transform:translateX(-50%) translateY(-57%);
    -o-transform:translateX(-50%) translateY(-57%);
    z-index: 8;
}
.turntable-bg .rotate {
    width: 85%;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translateX(-50%) translateY(-50%);
    -webkit-transform: translateX(-50%) translateY(-50%);
    -moz-transform: translateX(-50%) translateY(-50%);
    -o-transform: translateX(-50%) translateY(-50%);
}
