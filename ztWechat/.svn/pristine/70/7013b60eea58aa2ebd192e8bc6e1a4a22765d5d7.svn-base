/*幸运值 */
.pr .progress {
    width:60%;
    position:absolute;
    left:20%;
    top:0;
    height: 4vw;
    background:rgba(129,38,202,0.8);
    border-radius: 4vw;
    box-shadow:inset 0 .05rem .1rem #370f56;
    -webkit-box-shadow:inset 0 .05rem .1rem #370f56;
    -moz-box-shadow:inset 0 .05rem .1rem #370f56;
    -o-box-shadow:inset 0 .05rem .1rem #370f56;
}
.pr .progress .progressBar {
    position:absolute;
    left: 0.3vw;
    top: 0.3vw;
    height: 3.5vw;
    background:#eeca21;
    border-radius: 3.5vw;
    box-shadow: 0.2vw 0.2vw 0.3vw #895b07;
    -webkit-box-shadow:0.2vw 0.2vw 0.3vw #895b07;
    -moz-box-shadow:0.2vw 0.2vw 0.3vw #895b07;
    -o-box-shadow:0.2vw 0.2vw 0.3vw #895b07;
}
.pr .progress span {
    position: absolute;
    color:#532083;
    font-size: 4.5vw;
    top: -1vw;
}
.pr .progress .min {
    left:-4.5vw;
}
.pr .progress .max {
    right:-7vw;
}
.pr .progress .value {
    position:absolute;
    right:-5vw;
    top: 4vw;
    width: 7.5vw;
    height: 9vw;
    line-height: 9vw;
    background:url("../images/activity/value.png") no-repeat;
    background-size:90% 90%;
    font-size: 4vw;
    color:#a93c11;
    text-align: left;
    padding-left: 1.5vw;
}
/*无缝滚动样式*/
.scroll {
    overflow: hidden;
