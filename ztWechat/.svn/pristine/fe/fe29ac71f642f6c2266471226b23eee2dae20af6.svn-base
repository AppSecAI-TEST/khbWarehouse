/*基金公告*/
.fund-Area{
    margin-top: 0;
    padding: 0;
}
.fund-Area li{
    padding: .32rem 3%;
    border-bottom: .02rem solid #eee;
    position: relative;
}
.fund-Area li a{
    position: relative;
    display: block;
    overflow: hidden;
}
.fund-Area li .icon-arrow-right{
    font-size: .36rem;
    position:absolute;
    top: 50%;
    right: 0;
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);
}
.comment_inner{
    width: 90%;
    font-size: .28rem;
    color: #444;
    overflow: hidden;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
.alertLayer .tipsBox{
    width: 94%;
    margin: 0 auto;
}
.btnMaskArea a.btn-gray2{
    background: #bbb;
}
@media screen and (max-width: 320px) {
